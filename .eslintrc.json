{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-console": "off",
        "max-len": ["error", {
            "code": 120
        }]
    }
}
